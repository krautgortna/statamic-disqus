{
    "name": "krautgortna/statamic-disqus",
    "description": "This Statamic Addon adds a Disqus comment section to your Statamic views",
    "type": "addon",
    "license": "MIT",
    "require": {
      "php": ">= 7.0",
      "statamic/cms": "^3.0"
    },
    "keywords": [
        "statamic",
        "disqus",
        "comments",
        "forum"
    ],
    "autoload": {
        "psr-4": {
            "Krautgortna\\Disqus\\": "src"
        }
    },
    "authors": [
        {
            "name": "Manfred Milchrahm"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Disqus",
            "description": "Disqus addon"
        },
        "laravel": {
            "providers": [
                "Krautgortna\\Disqus\\ServiceProvider"
            ]
        }
    }
}
